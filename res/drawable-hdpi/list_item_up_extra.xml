<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >  
    <item>
	    <shape
	        android:innerRadius="2dp">
	        <solid android:color="@color/et_grey1"/>
           
	        <size android:width="250dp"
	              android:height="5dp"/> 
	        <padding android:bottom="0dp"
	                 android:left="1dp"
	                 android:top="0dp"
	                 android:right="0dp"/>
	    </shape>
    </item>
     
    <item>
	    <shape
	        android:innerRadius="2dp">
	        <gradient
				android:startColor="@color/et_white"
				android:centerColor="@color/et_white"
				android:endColor="@color/et_white"
				android:angle="270"/>
	    </shape>
    </item>
</layer-list>
